#include "eqMan.hxx"

Expression::Expression() {}
Expression::~Expression() {}

void Expression::convert(Stack rawInput) {
  NodeData temp;

  //perform the conversion
  while(rawInput.top()) {
    if(rawInput.top().type == OPERAND) {
      //the item is a number, add to stack
      temp.type=OPERAND;
      temp.payload=rawInput.pop().payload;
